https://practice.geeksforgeeks.org/problems/transpose-of-matrix-1587115621/1

void transpose(vector<vector<int> >& matrix, int n)
    { 
        //works only on square matrix
        for(int i=0;i<n;i++){
            for(int j=i+1; j<n ; j++){
                swap(matrix[i][j], matrix[j][i]);
            }
        }
    }
